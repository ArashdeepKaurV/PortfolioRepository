--  View to see Population with Covid

CREATE VIEW 
	Population_w_Covid
AS
	SELECT 
	location, 
	date,
	total_cases,
	population,
	ROUND(CAST(total_cases AS FLOAT) / CAST(population AS FLOAT)*100,2)  AS populationwCovid
FROM 
	CovidDeaths
ORDER BY 
